rrtm : rrtm.o rrtatm.o setcoef.o rtr.o rtreg.o taumol.o k_g.o extra.o\
util_sun.o rtrcld.o rtregcld.o cldprop.o
	pgf90 -fast -o  rrtm  rrtm.o rrtatm.o setcoef.o rtr.o\
  rtreg.o taumol.o k_g.o extra.o util_sun.o rtrcld.o rtregcld.o cldprop.o
#
rrtm.o: rrtm.f
	pgf90  -fast    -c  rrtm.f
#
rrtatm.o: rrtatm.f
	pgf90 -fast    -c  rrtatm.f
#
