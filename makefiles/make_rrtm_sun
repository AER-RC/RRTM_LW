	pgf90    -fast    -c  rtr.f
#
rtreg.o: rtreg.f
	pgf90    -fast    -c  rtreg.f
#
taumol.o: taumol.f
	pgf90 -fast  -c  taumol.f
#
k_g.o: k_g.f
	get SCCS/s.k_gB*.f
	pgf90 -fast  -c  k_g.f
#
extra.o: extra.f
	pgf90 -fast  -c  extra.f
#
util_sun.o: util_sun.f
	pgf90 -fast  -c  util_sun.f
#
rtrcld.o: rtrcld.f
	pgf90    -fast    -c  rtrcld.f
#
rtregcld.o: rtregcld.f
	pgf90    -fast    -c  rtregcld.f
#
